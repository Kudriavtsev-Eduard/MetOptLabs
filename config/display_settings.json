{
  "table": {
    "header": {
      "values": [
        "<b>Parameter</b>",
        "<b>Value</b>"
      ],
      "fill_color": "lightblue",
      "align": "center"
    },
    "fill_color": "lavender",
    "align": "left",
    "x_alignment": 0.05,
    "y_domain": [
      0.5,
      1
    ]
  },
  "graph": {
    "display_range_bounds": [
      -10,
      10
    ],
    "colorscale": "Sunsetdark",
    "level_line_indent": 5,
    "opacity": 0.5
  },
  "error_comparison": {
    "graph": {
      "scatter_mode": "markers+lines",
      "error_line_color": "MediumAquaMarine",
      "data_line_color": "Salmon",
      "error_graph_name": "Diff",
      "data_graph_name": "Data",
      "zero_line_dash": "dash"
    },
    "alignment": {
      "row_heights": [
        0.3,
        0.7
      ],
      "vertical_spacing": 0.05,
      "specs": [[{"type": "table"}], [{"type": "xy"}]],
      "subplot_titles": ["SGD parameters:", "Data and error graphs:"]
    }
  },
  "trace": {
    "default_marker": "circle",
    "special_marker": "diamond",
    "scatter_mode": "markers+lines",
    "marker_params": {
      "size": 7,
      "color": "royalblue"
    },
    "line_params": {
      "width": 7,
      "color": "royalblue"
    }
  }
}